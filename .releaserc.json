{
  "branches": [
    "master"
  ],
  "plugins": [
    "@semantic-release/commit-analyzer",
    "@semantic-release/release-notes-generator",
    [
      "@semantic-release/exec",
      {
        "prepareCmd": "dotnet build Meadow_Framework/Meadow_Framework.csproj --configuration Release"
      }
    ],
    [
      "@semantic-release/github",
      {
        "assets": [
          {
            "path": "output/*.nupkg",
            "label": "NuGet Package"
          }
        ]
      }
    ]
  ]
}
